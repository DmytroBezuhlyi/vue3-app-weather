{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "outDir": "build",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["server.ts"]
}
